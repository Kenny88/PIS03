<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pantalla_prinicpal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/intro"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/Save2Button"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="@string/Save2"
        android:textColor="@color/Blanco" />

    <Button
        android:id="@+id/Save1Button"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_above="@+id/Save2Button"
        android:layout_alignLeft="@+id/Save2Button"
        android:layout_marginBottom="24dp"
        android:text="@string/Save1"
        android:textColor="@color/Blanco" />

    <Button
        android:id="@+id/Save3Button"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/Save2Button"
        android:layout_below="@+id/Save2Button"
        android:layout_marginTop="24dp"
        android:text="@string/Save3"
        android:textColor="@color/Blanco" />

</RelativeLayout>